{
    "env": {
        "browser": true,
        "es2020":true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "func-names": ["error", "never"],
      "prefer-arrow-callback": 0,
      "space-before-function-paren": 0,
      "block-spacing": 0,
      "brace-style": 0,
      "no-param-reassign": 0,
      "no-plusplus": 0,
      "prefer-template": 0,
      "default-case": 0
    }
}
